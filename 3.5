#include <iostream>

int main() {
    char grade;
    std::cout << "Введите оценку (A, B, C, D, F): ";
    std::cin >> grade;

    int points = 0;
    switch (grade) {
        case 'A': points = 4; break;
        case 'B': points = 3; break;
        case 'C': points = 2; break;
        case 'D': points = 1; break;
        case 'F': points = 0; break;
        default: std::cout << "Неверная оценка.n"; return 1;
    }

    std::cout << points << "n";
    return 0;
}
