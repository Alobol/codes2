#include <iostream>

int main() {
    int month;
    std::cout << "Введите номер месяца (1-12): ";
    std::cin >> month;

    switch (month) {
        case 12: case 1: case 2: std::cout << "Зимаn"; break;
        case 3: case 4: case 5: std::cout << "Веснаn"; break;
        case 6: case 7: case 8: std::cout << "Летоn"; break;
        case 9: case 10: case 11: std::cout << "Осеньn"; break;
        default: std::cout << "Неверный номер месяца.n"; break;
    }

    return 0;
}
