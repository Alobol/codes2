#include <iostream>

int main() {
    double num1, den1, num2, den2;

    std::cout << "Введите числитель и знаменатель первой дроби: ";
    std::cin >> num1 >> den1;

    std::cout << "Введите числитель и знаменатель второй дроби: ";
    std::cin >> num2 >> den2;

    if (num1 * den2 > num2 * den1)
        std::cout << "Первая дробь большеn";
    else if (num1 * den2 < num2 * den1)
        std::cout << "Вторая дробь большеn";
    else
        std::cout << "Дроби равныn";

    return 0;
}
