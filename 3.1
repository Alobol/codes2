#include <iostream>

int main() {
    double num1, num2;
    char op;
    std::cin >> num1 >> op >> num2;

    if (op == '+') std::cout << num1 + num2;
    else if (op == '-') std::cout << num1 - num2;
    else if (op == '*') std::cout << num1 * num2;
    else if (op == '/' && num2 != 0) std::cout << num1 / num2;
    else std::cout << "Ошибка!";

    return 0;
}
