#include <iostream>

bool isLeapYear(int year) {
    return (year % 4 == 0 && year % 100 != 0) || (year % 400 == 0);
}

int main() {
    int year;
    std::cout << "Введите год: ";
    std::cin >> year;

    if (isLeapYear(year)) {
        std::cout << year << " - високосный" << std::endl;
    } else {
        std::cout << year << " - не високосный" << std::endl;
    }

    return 0;
}
